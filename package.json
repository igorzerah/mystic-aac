{
  "name": "tibia-website",
  "version": "1.0.0",
  "description": "A Tibia-themed website",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint . --ext .ts",
    "build:js": "tsc && terser public/js/home.js -c -m -o public/js/home.min.js && del /Q public\\js\\home.js",
    "build:all": "npm run build && npm run build:js"
  },
  "keywords": [
    "tibia",
    "nodejs",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/connect-redis": "^0.0.23",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.1",
    "@types/helmet": "^0.0.48",
    "@types/ioredis": "^4.28.10",
    "@types/method-override": "^3.0.0",
    "@types/multer": "^1.4.12",
    "@types/redis": "^4.0.10",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "connect-redis": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "dotenv-safe": "^9.1.0",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "helmet-csp": "^4.0.0",
    "ioredis": "^5.5.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "pg": "^8.13.3",
    "prisma": "^6.4.0",
    "rate-limiter-flexible": "^5.0.5",
    "redis": "^4.7.0",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "terser": "^5.39.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "type": "commonjs"
}
